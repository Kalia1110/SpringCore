<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
	<bean id="student" class="kcp.spring.beans.Student">
		<constructor-arg value="kalu charan panda" />
		<constructor-arg value="234" />
		<constructor-arg value="321" />
	</bean>
	<bean id="student1" class="kcp.spring.beans.Student">
		<constructor-arg value="kalu charan panda" index="0" />
		<constructor-arg value="234" index="2" />
		<constructor-arg value="321" index="1"/>
	</bean>
<bean id="student3" class="kcp.spring.beans.Student">
		<constructor-arg value="kalu charan panda" name="sName" />
		<constructor-arg value="234" name="sRollno" />
		<constructor-arg value="321" name="sMark"/>
	</bean>
</beans>
